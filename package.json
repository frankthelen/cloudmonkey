{
  "name": "cloudmonkey",
  "version": "1.0.0-alpha.0",
  "description": "A little infrastructure testing library for AWS / experimental in an early stage",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/frankthelen/cloudmonkey.git"
  },
  "keywords": [
    "cloud",
    "test",
    "aws"
  ],
  "author": "Frank Thelen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frankthelen/cloudmonkey/issues"
  },
  "homepage": "https://github.com/frankthelen/cloudmonkey#readme",
  "scripts": {
    "lint": "eslint . --ignore-path ./.eslintignore",
    "test": "NODE_ENV=test mocha --recursive test",
    "cover": "NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha --exit --recursive test",
    "coveralls": "npm run cover && nyc report --reporter=lcovonly && cat ./coverage/lcov.info | coveralls",
    "preversion": "npm run lint && npm test"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "aws-sdk": "^2.196.0",
    "bluebird": "^3.5.1",
    "config": "^1.29.4",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-should-promised": "^2.0.0",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.0",
    "nyc": "^11.4.1",
    "sinon": "^4.3.0",
    "sinon-chai": "^2.14.0"
  }
}
